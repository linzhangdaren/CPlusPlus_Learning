    "        self.名字=名字\n",
    "        self.性别=性别\n",
    "        self.颜色=颜色\n",
    "\n",
    "#实例化对象\n",
    "猫=动物(\"小白\",\"雌性\",\"黄色\")\n",
    "print(猫.名字)\n"
